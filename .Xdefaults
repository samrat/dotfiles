emacs.font: Inconsolata-dz for Powerline-12

URxvt.scrollBar: off
URxvt*termName: screen-256color
URxvt.imLocale: en_US.utf8
URxvt.foreground:white
URxvt.background:black
! URxvt.iconFile:    /usr/share/icons/gnome/48x48/apps/terminal.png
URxvt.perl-ext-common:  default,matcher
URxvt.urlLauncher:      chromium
URxvt.matcher.button:   1

URxvt*font:                 xft:Mensch for Powerline:regular:size=11
URxvt*imFont:               xft:Mensch for Powerline:regular:size=11
URxvt*boldFont:             xft:Mensch for Powerline:bold:size=11
URxvt*italicFont:           xft:Mensch for Powerline:italic:size=11
URxvt*boldItalicFont:       xft:Mensch for Powerline:bold:italic:size=11

! black
URxvt*color0  : #000000
URxvt*color8  : #000000
! red
URxvt*color1  : #E52222
URxvt*color9  : #E52222
! green
URxvt*color2  : #A6E32D
URxvt*color10 : #A6E32D
! yellow
URxvt*color3  : #FC951E
URxvt*color11 : #FC951E
! blue
URxvt*color4  : #BF8DFF
URxvt*color12 : #BF8DFF
! magenta
URxvt*color5  : #FA250F
URxvt*color13 : #FA250F
! cyan
URxvt*color6  : #67D9F0
URxvt*color14 : #67D9F0
! white
URxvt*color7  : #F2F2F2
URxvt*color15 : #F2F2F2
